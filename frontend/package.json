{
  "homepage": "https://rohanlakade.github.io/fullstack-todo-app/frontend",
  "name": "frontend",
  "version": "1.0.0",
  "description": "Fullstack Todo App using React js (Webpack), Node js, Mysql",
  "license": "ISC",
  "author": "Rohan Lakade",
  "main": "index.js",
  "scripts": {
    "start": "cross-env DOTENV_CONFIG_PATH=.env.local webpack serve --config webpack.dev.js --mode development",
    "build:github": "cross-env DOTENV_CONFIG_PATH=.env.github webpack --config webpack.prod.js --mode production",
    "build:vercel": "cross-env DOTENV_CONFIG_PATH=.env.vercel webpack --config webpack.prod.js --mode production",
    "serve": "serve -s dist",
    "deploy:github": "npm run build:github && gh-pages -d dist"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.2.0",
    "sass": "^1.85.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "babel-loader": "^9.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "dotenv-webpack": "^8.1.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.3.0",
    "html-webpack-plugin": "^5.6.3",
    "kill-port": "^2.0.1",
    "mini-css-extract-plugin": "^2.9.2",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
}
